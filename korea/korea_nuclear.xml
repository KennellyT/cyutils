        <fuel_incommods>  <val>uox</val>                   </fuel_incommods>
        <fuel_outcommods> <val>uox_waste</val>             </fuel_outcommods>
        <fuel_prefs>      <val>1.0</val>                   </fuel_prefs>
        <cycle_time>18</cycle_time>
        <refuel_time>2</refuel_time>
        <assem_size>523.4</assem_size>
